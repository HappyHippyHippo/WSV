{
    "name": "happyhippyhippo/wsv",
    "description": "Whitespace Separated Value library",
    "type": "library",
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "HappyHippyHippo\\WSV\\": "src/",
            "HappyHippyHippo\\WSV\\tests\\": "test/"
        }
    },
    "authors": [
        {
            "name": "Carlos Dias",
            "email": "carlos.dias@turbinekreuzberg.com"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/happyhippyhippo/textio"
        }
    ],
    "require": {
        "php": "^8.1",
        "ext-mbstring": "*",
        "ext-intl": "*",
        "happyhippyhippo/textio": "dev-master@dev"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.5",
        "mikey179/vfsstream": "1.6.11",
        "squizlabs/php_codesniffer": "^3.8",
        "friendsofphp/php-cs-fixer": "^3.49",
        "phpstan/phpstan": "^1.10"
    }
}
